{
  "name": "parcel-react-ts-example",
  "description": "Parcel TypeScript simple example",
  "version": "v0.0.2",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --open",
    "build": "parcel build src/index.html",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "serve": "serve ./dist",
    "deploy": "cd dist &&vercel"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.2",
    "parcel": "latest",
    "postcss": "^8.4.6",
    "serve": "^13.0.2",
    "typescript": "^4.5.5",
    "vercel": "^24.0.0"
  }
}
